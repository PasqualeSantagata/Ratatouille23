<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/bg_login">

    <ImageView
        android:id="@+id/imageViewMenuDash"
        android:layout_width="154dp"
        android:layout_height="156dp"
        android:layout_marginStart="51dp"
        android:layout_marginTop="35dp"
        android:layout_marginBottom="149dp"
        app:layout_constraintBottom_toTopOf="@+id/buttonEsciDash"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/im_menu" />

    <ImageView
        android:id="@+id/imageViewAnalyticsDash"
        android:layout_width="154dp"
        android:layout_height="156dp"
        android:layout_marginStart="85dp"
        android:layout_marginTop="35dp"
        android:layout_marginBottom="220dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toEndOf="@+id/imageViewMenuDash"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/im_analytics" />

    <ImageView
        android:id="@+id/imageViewNewUserDash"
        android:layout_width="154dp"
        android:layout_height="156dp"
        android:layout_marginStart="85dp"
        android:layout_marginTop="35dp"
        android:layout_marginEnd="51dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/imageViewAnalyticsDash"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/im_new_user" />

    <Button
        android:id="@+id/buttonEsciDash"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="50dp"
        android:layout_marginBottom="25dp"
        android:backgroundTint="@color/white"
        android:text="@string/esci"

        android:textColor="@color/black"
        app:cornerRadius="@android:dimen/app_icon_size"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="50dp"
        android:layout_marginTop="12dp"
        android:layout_marginBottom="106dp"
        android:fontFamily="@font/inter"
        android:text="GESTIONE MENÃ™"
        android:textColor="@color/black"
        android:textSize="20sp"
        app:layout_constraintBottom_toTopOf="@+id/buttonEsciDash"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageViewMenuDash" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="91dp"
        android:layout_marginTop="12dp"
        android:layout_marginEnd="114dp"
        android:fontFamily="@font/inter"
        android:text="ANALYTICS"
        android:textColor="@color/black"
        android:textSize="20sp"
        app:layout_constraintEnd_toStartOf="@+id/textView5"
        app:layout_constraintStart_toEndOf="@+id/textView2"
        app:layout_constraintTop_toBottomOf="@+id/imageViewAnalyticsDash" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="11dp"
        android:layout_marginEnd="55dp"
        android:layout_marginBottom="180dp"
        android:fontFamily="@font/inter"
        android:text="CREA UTENZA"
        android:textColor="@color/black"
        android:textSize="20sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageViewNewUserDash" />

</androidx.constraintlayout.widget.ConstraintLayout>