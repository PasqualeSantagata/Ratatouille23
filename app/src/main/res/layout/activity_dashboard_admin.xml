<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/bg_login">

    <ImageView
        android:id="@+id/imageViewMenuDash"
        android:layout_width="223dp"
        android:layout_height="235dp"
        android:layout_marginStart="96dp"
        android:layout_marginTop="106dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/im_menu" />

    <ImageView
        android:id="@+id/imageViewAnalyticsDash"
        android:layout_width="223dp"
        android:layout_height="235dp"
        android:layout_marginStart="210dp"
        android:layout_marginTop="106dp"
        android:layout_marginEnd="193dp"
        app:layout_constraintEnd_toStartOf="@+id/imageViewNewUserDash"
        app:layout_constraintStart_toEndOf="@+id/imageViewMenuDash"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/im_analytics" />

    <ImageView
        android:id="@+id/imageViewNewUserDash"
        android:layout_width="223dp"
        android:layout_height="235dp"
        android:layout_marginTop="106dp"
        android:layout_marginEnd="112dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/im_new_user" />

    <Button
        android:id="@+id/buttonEsciDash"
        android:layout_width="180dp"
        android:layout_height="60dp"
        android:layout_marginStart="200dp"
        android:layout_marginEnd="1050dp"
        android:layout_marginBottom="100dp"
        android:backgroundTint="@color/white"

        android:text="@string/esci"
        android:textColor="@color/black"
        app:cornerRadius="@android:dimen/app_icon_size"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="122dp"
        android:layout_marginTop="21dp"
        android:fontFamily="@font/inter"
        android:text="GESTIONE MENÃ™"
        android:textColor="@color/black"
        android:textSize="20sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageViewMenuDash" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="295dp"
        android:layout_marginTop="13dp"
        android:fontFamily="@font/inter"
        android:text="ANALYTICS"
        android:textColor="@color/black"
        android:textSize="20sp"
        app:layout_constraintStart_toEndOf="@+id/textView2"
        app:layout_constraintTop_toBottomOf="@+id/imageViewAnalyticsDash" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="289dp"
        android:layout_marginTop="13dp"
        android:layout_marginEnd="154dp"
        android:fontFamily="@font/inter"
        android:text="CREA UTENZA"
        android:textColor="@color/black"
        android:textSize="20sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/textView4"
        app:layout_constraintTop_toBottomOf="@+id/imageViewNewUserDash" />

</androidx.constraintlayout.widget.ConstraintLayout>